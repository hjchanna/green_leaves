	<parameter name="FROM_DATE" class="java.util.Date"/>
	<parameter name="TO_DATE" class="java.util.Date"/>
	<parameter name="VEHICLE" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
	concat('[', lpad(m_route.index_no,5,0), '] ', m_route.name) as route , 
	t_green_leaves_receive.vehicle,
	t_green_leaves_receive.date as date,
	m_vehicle.vehicle_no as 'vehicle no',
	sum(t_green_leaves_receive_detail.normal_leaves_quantity) as quantity,
	m_route.transport_deduction_rate as transport_rate,
	sum(t_green_leaves_receive_detail.normal_leaves_quantity)*m_route.transport_deduction_rate as amount
	
from
	t_green_leaves_receive_detail
	left join t_green_leaves_receive on t_green_leaves_receive.index_no = t_green_leaves_receive_detail.green_leaves_receive
	left JOIN m_route on m_route.index_no = t_green_leaves_receive.route
	left join m_vehicle on m_vehicle.index_no = t_green_leaves_receive.vehicle
where
	 t_green_leaves_receive.date between  $P{FROM_DATE} and   $P{TO_DATE} 
	and ( $P{VEHICLE}  is null or t_green_leaves_receive.vehicle=  $P{VEHICLE} )
	and t_green_leaves_receive.`status`!='DELETED'
GROUP BY t_green_leaves_receive.vehicle,t_green_leaves_receive.date
ORDER BY t_green_leaves_receive.vehicle,t_green_leaves_receive.date]]>
	</queryString>
